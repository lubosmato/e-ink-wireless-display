    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.190",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
