    "@types/apollo-upload-client": "^17.0.2",
    "apollo-upload-client": "^17.0.0",
    "cookie": "^0.4.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "mdi-svelte": "^1.1.2"
